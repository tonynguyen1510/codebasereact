{
    "presets": [
		"next/babel",
        "react",
        "es2015",
        "stage-0"
    ],
    "plugins": [
        [
            "transform-runtime",
            {
                "polyfill": false,
                "regenerator": true
            }
        ],
		"transform-decorators-legacy",
        [
            "module-resolver",
            {
                "root": [
                    "./"
                ],
                "alias": {
					"src": "./src",
					"node_modules": "./node_modules"
                }
            }
        ],
        [
            "import",
            {
                "libraryName": "antd",
                "style": false
            }
        ]
    ]
}
